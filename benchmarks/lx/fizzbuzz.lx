fn fizzbuzz(n) {
  // checksum: 0=number, 1=Fizz, 2=Buzz, 3=FizzBuzz
  let acc = 0
  for let i = 1; i <= n; i = i + 1 {
    let a = i % 3 == 0
    let b = i % 5 == 0
    if a and b {
      acc = acc + 3
    } else if a {
      acc = acc + 1
    } else if b {
      acc = acc + 2
    }
  }
  acc
}

let raw = Lx.args[3]
let n = raw and tonumber(raw) or 0
print(fizzbuzz(n))
