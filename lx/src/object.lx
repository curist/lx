let ObjType = enum { CLOSURE, FUNCTION, NATIVE, STRING, UPVALUE, HASHMAP, ENUM, ARRAY }

fn Object(kind, value) {.{
  kind,
  value,
}}

fn ObjectFunction(function) {
  Object(ObjType.FUNCTION, function)
}

fn ObjectString(string) {
  Object(ObjType.STRING, string)
}

return .{ ObjType, Object, ObjectFunction, ObjectString }
