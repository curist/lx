let initScanner = import "src/passes/frontend/scanner.lx"
let types = import "src/types.lx"
let ModuleResolution = import "src/module_resolution.lx"
let TOKEN = types.TOKEN

let resolver = ModuleResolution.forEntry("test/stub/sink.lx")
let scanner = initScanner(resolver.slurp("test/stub/sink.lx"))

for true {
  let tok = scanner.scanToken()

  println(tok)
  if tok.type == TOKEN.EOF {
    break
  }
}
